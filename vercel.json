{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/products",
      "dest": "api/index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/categories",
      "dest": "api/index.js"
    },
    {
      "src": "/categories/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/brands",
      "dest": "api/index.js"
    },
    {
      "src": "/brands/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/users",
      "dest": "api/index.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/cart",
      "dest": "api/index.js"
    },
    {
      "src": "/cart/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/orders",
      "dest": "api/index.js"
    },
    {
      "src": "/orders/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/auth",
      "dest": "api/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/webhook",
      "dest": "api/index.js"
    },
    {
      "src": "/create-payment-intent",
      "dest": "api/index.js"
    },
    {
      "src": "/add",
      "dest": "build/index.html"
    },
    {
      "src": "/",
      "dest": "frontend/build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "frontend/build/$1"
    }
  ]
}